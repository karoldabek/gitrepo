<script>
// Edit your script here
var a = parseInt(prompt('Podaj bok a: ')); // parseInt daje że to są liczby tero
var b = parseInt(prompt('Podaj bok b: '));
var c = parseInt(prompt('Podaj bok c: '));
document.write(a + b + c);
if (+b>c && a+c>b && b+c>a) {
	window.alert('da się');
}	else {
		window.alert('Nie da się! Frajerze')
}
</script>
